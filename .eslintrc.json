{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],

  "overrides": [],

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "plugins": ["react", "prettier", "import"],

  "rules": {
    "react/react-in-jsx-scope": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": ["off"]
  }
}
